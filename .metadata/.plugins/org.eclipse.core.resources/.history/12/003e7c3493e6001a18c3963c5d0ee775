package quiz;

public class Quiz22 {
	
	public static void main(String[] args) {
		
		//1. 7~100까지 정수 중 7의 배수를 가로로 출력
		for(int i = 7; i < 101; i++) {
			if(i%7 == 0) {
				System.out.printf("%d ", i);
			}
		}
		
		
