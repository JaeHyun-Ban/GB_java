package day05;

public class BreakEx03 {
	
	public static void main(String[] args) {
		
		for(char u = 'A'; u <= 'Z'; u++) {
			
			for(char l = 'a'; l <= 'z'; l++) {
				System.out.println(u + "-" + l);
				
				if(l == 'c') {
					System.out.println();
					break;
				}
				
			}//내부 for
		
		}//외부 for
		
		
		
		
		
	}
}
