package quiz;

public class Quiz27 {
	
	public static void main(String[] args) {
		
		//Quiz13-1ë²ˆ
		
		for(int i = 1; i < 10; i++) {
//			if()
			
			for(int j = 1; j < 10; j++) {
				System.out.printf("%d x %d = %d\t", j, i, i*j);
			}
			
			
		}
		
		
		
		
		
		
		
		
		
		
	}
	
}































