package quiz;

import java.util.Arrays;

public class Quiz30 {

	public static void main(String[] args) {
		
		int[] arr = {5, 23, 1, 43, 100, 200, 40};// 길이가 7
		
		//자료구조 
		for(int i = 0;  i < arr.length; i++) {
			
			for(int j = i+1; j < arr.length; j++) {
				if(arr[i] > arr[i+1]) {
					int tmp = arr[i];
					arr[i] = arr[i+1];
					arr[i+1] = tmp;
				}	
			}
		}
		
		System.out.println(Arrays.toString(arr));
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}
}





