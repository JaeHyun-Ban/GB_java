package quiz;

public class Quiz27 {
	
	public static void main(String[] args) {
		
		//Quiz13-1ë²ˆ
		
		for(int k = 1; k < 10; k+=3) {
			for(int i = 1; i < 10; i++) {
				
				for(int j = 1; j < 10; j++) {
					System.out.printf("%d x %d = %d\t", j, i, i*j);
				}
				System.out.println();
				
			}
			System.out.println();
		}
		
		
		
		
		
		
		
		
		
		
		
	}
	
}































