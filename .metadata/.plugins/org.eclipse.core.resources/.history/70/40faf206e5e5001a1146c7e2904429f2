package day03;

import java.util.Scanner;

public class WhileEx02 {
	public static void main(String[] args) {
		
		//입력받은 수가 소수 인지 판별
		
		Scanner sc = new Scanner(System.in);
		
		System.out.print("정수 입력> ");
		int num = sc.nextInt();
		int count = 0;
		while(count < 2) {
			
			for(int i = 2; i < num+1;i++) {
				if(num%i == 0) {
					count++;
				}
			}
		}
		System.out.println(num);
		
		
		
		
		
		
	}
}




















