package quiz;

public class Quiz27 {
	
	public static void main(String[] args) {
		
		//Quiz13-1ë²ˆ
		
		
		for(int i = 1; i < 10; i+=3) {
			
			for(int j = 1; j < 10; j++) {
				
				System.out.printf("%d x %d = %d\t%d x %d = %d\t%d x %d = %d\t\n", 
									i, j, i*j, i+1, j, (i+1)*j, i+2, j, (i+2)*j;
				
			}
			System.out.println();
			
		}
		
		
		
		
		
		
		
		
		
		
	}
	
}































