package day03;

import java.util.Scanner;

public class WhileEx02 {
	public static void main(String[] args) {
		
		//입력받은 수가 소수 인지 판별
		
		Scanner sc = new Scanner(System.in);
		
		System.out.print("정수 입력> ");
		int num = sc.nextInt();
		int count = 0;
		int x = 2;
		while( num % x != 0) { //나머지가 있다면
			
			x++;
		}
		System.out.println(num);
		
		
		
		
		
		
	}
}




















